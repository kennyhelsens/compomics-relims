<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.compomics.relims.gui.RelimsGUI">
  <splitpane id="7bf1b" binding="splitContent">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties>
      <oneTouchExpandable value="true"/>
      <orientation value="0"/>
      <resizeWeight value="0.5"/>
    </properties>
    <border type="none"/>
    <children>
      <grid id="27dc6" binding="jpanContent" layout-manager="GridBagLayout">
        <constraints>
          <splitpane position="left"/>
        </constraints>
        <properties>
          <enabled value="true"/>
          <preferredSize width="1000" height="600"/>
        </properties>
        <border type="none"/>
        <children>
          <scrollpane id="afbc6" binding="scrollLayout">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="2" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
              <gridbag weightx="1.0" weighty="1.0"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <component id="8ff18" class="javax.swing.JTextArea" binding="txtMessagePanel">
                <constraints/>
                <properties>
                  <editable value="false"/>
                  <selectionEnd value="0"/>
                  <selectionStart value="0"/>
                  <text value=""/>
                </properties>
              </component>
            </children>
          </scrollpane>
          <grid id="59bc6" binding="jpanButtons" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="2" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
              <gridbag weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <component id="889f8" class="javax.swing.JButton" binding="btnStart">
                <constraints/>
                <properties>
                  <text value="Start"/>
                </properties>
              </component>
              <component id="3e9ea" class="javax.swing.JButton" binding="iStopButton" default-binding="true">
                <constraints/>
                <properties>
                  <text value="Stop"/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
      <grid id="6329d" binding="jpanOptions" layout-manager="GridLayoutManager" row-count="1" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints>
          <splitpane position="right"/>
        </constraints>
        <properties/>
        <border type="none"/>
        <children>
          <component id="59d64" class="javax.swing.JButton" binding="btnSaveOptions">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Save"/>
            </properties>
          </component>
          <scrollpane id="43d96">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <component id="28613" class="javax.swing.JTable" binding="tblOptions">
                <constraints/>
                <properties>
                  <fillsViewportHeight value="false"/>
                  <font size="9"/>
                  <selectionBackground color="-3355444"/>
                </properties>
              </component>
            </children>
          </scrollpane>
        </children>
      </grid>
    </children>
